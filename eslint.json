{
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-var": "error",
      "prefer-const": "error",
      "prefer-arrow-callback": "error",
      "no-const-assign": "error",
      "no-duplicate-imports": "error"
    }
  }
